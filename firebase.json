{
  "hosting": [
    {
      "site": "fem-readup",
      "public": "dist/fem-readup-search-engine-ng",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }, {
      "site": "fem-readup-preview",
      "public": "dist/fem-readup-search-engine-ng",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
